class Solution {
    public int differenceOfSum(int[] nums) {
        int sum1=0,sum2=0;
        int n=nums.length;
        for(int i=0;i<n;i++){
            sum1+=nums[i];
        }
        for(int j=0;j<n;j++){
            int x=nums[j];
            while(x>0){
                sum2+=x%10;
                x/=10;
            }
        }
        return sum1-sum2;
    }
}
