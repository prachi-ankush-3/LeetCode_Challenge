import java.util.*;

class Solution {
    public int heightChecker(int[] heights) {
        int[] expected = heights.clone();  // make copy
        Arrays.sort(expected);             // sort copy
        int count = 0;

        for (int i = 0; i < heights.length; i++) {
            if (heights[i] != expected[i])  // compare
                count++;
        }

        return count;
    }
}
